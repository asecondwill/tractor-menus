<ul>
<% links.each do |link| %>
    <li data-id="<%= link.id %>"
        data-type='<%= link.class %>'
        data-title='<%= link.to_s %>'
        data-link='<%= link.link %>'
        data-linksearch='<%= link.link_search %>'
        data-record="<%= "#{link.class.to_s}:#{link.to_s}" %>"
        data-search-target="option"
        data-action="click->menuitem#selectOption">
      /<%= link.slug %>
      <div class="details">
        <%= "#{link.class.to_s}:#{link.to_s}" %>
      </div>
    </li>
<% end %>
</ul>
